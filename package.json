{
  "name": "gatsby-plugin-intl-contentful",
  "description": "Gatsby plugin to add react-intl fed with Contentful messages data",
  "version": "0.3.3",
  "author": "Arkadiusz Bachorski <contact@arkadiuszbachorski.com>",
  "bugs": {
    "url": "https://github.com/arkadiuszbachorski/gatsby-plugin-intl-contentful"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@formatjs/intl-pluralrules": "^1.5.2",
    "@formatjs/intl-relativetimeformat": "^4.5.9",
    "browser-lang": "^0.1.0",
    "contentful": "^7.14.6",
    "intl": "^1.2.5",
    "react-intl": "^3.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "babel-preset-gatsby-package": "^0.2.16",
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  },
  "homepage": "https://github.com/arkadiuszbachorski/gatsby-plugin-intl-contentful",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-intl",
    "gatsby-plguin-intl-contentful",
    "contentful",
    "i18n",
    "intl",
    "react-intl"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "repository": "https://github.com/arkadiuszbachorski/gatsby-plugin-intl-contentful",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write {src,__tests__}/**/*.{js,jsx}",
    "test": "jest",
    "clean": "rimraf *.js"
  }
}
